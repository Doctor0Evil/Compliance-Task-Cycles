      "action": "/#/vault"
    },
    {
      "trigger": "iom://party",
      "description": "Open real-time Discord Hunt Party sync",
      "action": "/#/party"
    },
    {
      "trigger": "iom://onboard",
      "description": "Show onboarding wizard",
      "action": "/#/onboard"
    },
    {
      "trigger": "iom://help",
      "description": "Show quick guide and feature tour",
      "action": "/#/help"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["comet-utils.js"],
